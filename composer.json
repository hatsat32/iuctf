{
	"name": "iucyber/iuctf",
	"type": "project",
	"description": "A ctf platform for everyone.",
	"homepage": "https://gitlab.com/iucyber/iuctf",
	"license": "MIT",
	"require": {
		"php": "^7.3||^8.0",
		"codeigniter4/framework": "^4.1",
		"codeigniter4/translations": "^4.1",
		"myth/auth": "^1.0",
		"league/commonmark": "^1.6",
		"nelexa/zip": "^3.3"
	},
	"require-dev": {
		"fakerphp/faker": "^1.9",
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^9.1"
	},
	"suggest": {
		"ext-fileinfo": "Improves mime type detection for files"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Config\\": "app/Config"
		},
		"exclude-from-classmap": [
			"**/Database/Migrations/**"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Support\\": "tests/_support"
		}
	},
	"scripts": {
		"test": "phpunit"
	},
	"support": {
		"source": "https://gitlab.com/iucyber/iuctf"
	}
}
